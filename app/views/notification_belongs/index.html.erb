<p id="notice"><%= notice %></p>

<h1>Notification Belongs</h1>

<table>
  <thead>
    <tr>
      <th>Readed</th>
      <th>User</th>
      <th>Notification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notification_belongs.each do |notification_belong| %>
      <tr>
        <td><%= notification_belong.readed %></td>
        <td><%= notification_belong.user_id %></td>
        <td><%= notification_belong.notification_id %></td>
        <td><%= link_to 'Show', notification_belong %></td>
        <td><%= link_to 'Edit', edit_notification_belong_path(notification_belong) %></td>
        <td><%= link_to 'Destroy', notification_belong, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification Belong', new_notification_belong_path %>
